    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^8.0.20",
    "@types/socket.io": "^2.1.4",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "mockdate": "^2.0.5",
